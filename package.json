{
	"name": "any-auth",
	"version": "1.0.0",
	"description": "Library for adding different auth providers with ease in a website",
	"main": "./dist/server/index.js",
	"module": "./dist/client/index.mjs",
	"types": "./dist/client/index.d.ts",
	"scripts": {
		"build": "tsup --config tsup.config.ts"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE",
		"CHANGELOG.md"
	],
	"keywords": [
		"auth",
		"google auth",
		"github auth",
		"microsoft auth",
		"linkedin auth",
		"salesforce auth",
		"discord auth",
		"spotify auth",
		"amazon auth",
		"slack auth",
		"twitch auth",
		"facebook auth",
		"x auth",
		"custom auth",
		"auth provider",
		"auth library",
		"oauth"
	],
	"author": "geniusmind9999",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^22.10.2",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	},
	"exports": {
		".": {
			"import": "./dist/client/index.mjs",
			"require": "./dist/server/index.js"
		}
	}
}
